{
    "version": 2,
    "builds":[
        {
            "src":"server.js",
            "use":"@vercel/node"
        },{
            "src":"src/**/*",
            "use":"@vercel/next"
        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"server.js"
        },{
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}