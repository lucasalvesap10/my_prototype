{
    "version": 2,
    "builds":[
        {
            "src":"server.js",
            "use":"@vercel/node"
        },{
            "src":"package.json",
            "use":"@vercel/next"
        },
        {
            "src":"postcss.config.js",
            "use":"postcss"
        },
        {
            "src":"postcss.config.js",
            "use":"tailwind"
        },
        {
            "src":"postcss.config.js",
            "use":"autoprefixer"
        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"server.js"
        },{
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}